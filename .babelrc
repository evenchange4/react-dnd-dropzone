{
  "presets": [
    "babel-preset-react-app",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            ">1%",
            "last 4 versions",
            "Firefox ESR",
            "not ie < 11"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-add-module-exports"
  ]
}
